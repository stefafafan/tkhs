{
    "name": "stefafafan/tkhs",
    "description": "tkhs is a simple tool to generate a presentable artifact in a Takashi-style.",
    "license": "MIT",
    "scripts": {
        "stan": "php ./vendor/bin/phpstan analyse --memory-limit 1G -c phpstan.neon"
    },
    "config": {
        "platform": {
            "php": "8.3.4"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "require": {
        "symfony/console": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/finder": "^7.0",
        "tecnickcom/tcpdf": "^6.7"
    }
}
